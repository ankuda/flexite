config = new function () {
    var self = this,
        paths = {
            edit_entry: decodeURIComponent("<%= edit_entry_path('?') %>")
        };

    this.editEntry = function(id) {
        event.stopPropagation();
        $flexite.get(getRoute("edit_entry", [id]))
    }

    function getRoute(name, params) {
        var path = paths[name];

        params.forEach(function (param) {
            path = path.replace('?', param)
        });

        return path;
    }
};
